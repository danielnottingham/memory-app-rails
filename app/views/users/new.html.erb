<h1>New User Form</h1>

<%= form_with do |form| %>
    <div class="field">
        <%= form.label :name %>
        <%= form.text_field :name %>
    </div>
    <div class="field">
        <%= form.label :phone %>
        <%= form.text_field :phone %>
    </div>
    <div class="actions">
        <%= form.submit 'Add' %>
    </div>

<% end %>
<%= link_to 'Back', '/users' %>
